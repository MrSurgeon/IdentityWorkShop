@model List<AppUser>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Index</h1>
<table class="table table-bordered ">
    <thead>
        <tr>
            <td>User Id</td>
            <td>User Name</td>
            <td>Email</td>
        </tr>
    </thead>
    @if (!Model.Any())
    {
        <tr>
            <td colspan="3">Hiç bir üye bulunmamaktadır !</td>
        </tr>
    }
    else
    {
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.UserName</td>
                <td>@item.Email</td>
            </tr>
        }

    }
    <tbody>
    </tbody>
</table>

